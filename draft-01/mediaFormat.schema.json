{
    "$id": "https://skystandards.hns.siasky.net/draft-01/mediaFormat.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "An available media format listed in a media object",
    "type": "object",
    "required": [
        "ext",
        "url"
    ],
    "properties": {
        "ext": {
            "type": "string",
            "description": "file extension of this media format"
        },
        "url": {
            "type": "string",
            "format": "uri"
        }
    }
}